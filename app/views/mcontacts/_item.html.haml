- content_tag_for(:tr, mcontact, :class => cycle(:odd, :even)) do
  %td.email= h mcontact.try(:email)
  %td.country_id= h mcontact.try(:country_id)
  %td.state= h mcontact.try(:state)
  %td.city= h mcontact.try(:city)
  %td.street1= h mcontact.try(:street1)
  %td.street2= h mcontact.try(:street2)
  %td.zip= h mcontact.try(:zip)
  %td.telnum1= h mcontact.try(:telnum1)
  %td.telnum1_type= h mcontact.try(:telnum1_type)
  %td.telnum2= h mcontact.try(:telnum2)
  %td.telnum2_type= h mcontact.try(:telnum2_type)
  %td.webpage1= h mcontact.try(:webpage1)
  %td.webpage1_type= h mcontact.try(:webpage1_type)
  %td.webpage2= h mcontact.try(:webpage2)
  %td.webpage2_type= h mcontact.try(:webpage2_type)
  %td.short_description= h mcontact.try(:short_description)
  %td.actions
    = link_to 'Show', mcontact_path(mcontact)
    |
    = link_to 'Edit', edit_mcontact_path(mcontact)
    |
    = link_to 'Destroy', mcontact, :confirm => 'Are you sure?', :method => :delete