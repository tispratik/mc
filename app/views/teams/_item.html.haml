- content_tag_for(:tr, team, :class => cycle(:odd, :even)) do
  %td.name= h team.try(:name)
  %td.organization_id= h team.try(:organization_id)
  %td.owner_user_id= h team.try(:owner_user_id)
  %td.actions
    = link_to 'Show', team_path(team)
    |
    = link_to 'Edit', edit_team_path(team)
    |
    = link_to 'Destroy', team, :confirm => 'Are you sure?', :method => :delete